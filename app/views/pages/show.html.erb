  <div id = "image-clipboard">
    <%= image_tag "vector-clipboard.png" %>
  </div>

  <div class='feedback'>
    <div class='feedback_text'>
      <div id ='accordion'>
      
        <h3> Title <%= image_tag "check1.png", :class => 'title_check' %></h3>
        <div>
          <p class='main_messages'><%= title_keywords_messages.html_safe %>
          </p>
        </div>
        <h3> URL <%= image_tag "check1.png",:class => 'title_check' %></h3>
        <div>
          <p class='url_messages'><%= url_keywords_messages.html_safe %>
          </p>
        </div>
        <h3> Word Count <%= image_tag "check1.png", :class => 'title_check' %></h3>
        <div>
          <p class='word_count_messages'><%= word_count_messages.html_safe %>
          </p>
        </div>
        <h3> Header Tags  <%= image_tag "check1.png", :class => 'title_check' %></h3>
        <div>
          <span class='h1_messages'><%= h1_messages.html_safe %>
          </span>
        </div>
        <h3> Images <%= image_tag "check1.png", :class => 'title_check' %></h3>
        <div>
          <p class='image_messages'><%= image_messages %>
          </p>
        </div>
        <h3> Links <%= image_tag "check1.png", :class => 'title_check' %></h3>
        <div>
          <p class='link_messages'><%= link_messages %>
          </p>
        </div>
        <h3> Content Body <%= image_tag "check1.png" %></h3>
        <div>
          <span class='content_body_messages'><%= content_body_messages %>
          </span>
        </div>
      </div>
    </div>
  </div><br>

  <div class='article_content'>
    <h2>ORIGINAL ARTICLE</h2>
    <%= @page.content.html_safe %>
  </div>






